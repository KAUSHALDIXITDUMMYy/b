' />\n\t\t\t\t)}\n\t\t\t\trenderThumbVertical={(props) => (\n\t\t\t\t\t<div {...props} className='fbx-thumb fbx-thumb-vertical' />\n\t\t\t\t)}\n\t\t\t>\n\t\t\t\t<div className='fbx-scrollable-content' ref={contentRef}>\n\t\t\t\t\t{children && typeof children === 'function'\n\t\t\t\t\t\t? (children as any)(update)\n\t\t\t\t\t\t: children}\n\t\t\t\t</div>\n\t\t\t</Scrollbars>\n\t\t</div>\n\t);\n});\n\nexport default Scrollable;\n","/**\n * External dependencies\n */\nimport { debounce } from 'lodash';\nimport { Id } from 'flexsearch-ts';\nimport classnames from 'classnames';\nimport React, { FC, useEffect, useMemo, useRef, useState } from 'react';\n\n/**\n * Internal dependencies\n */\nimport { getIndex, performSearchApiCall } from 'utils';\nimport { withConfig, withConfigProps } from 'HOC/withConfig';\nimport { withPlayerState, withPlayerStateProps } from 'HOC/withPlayerState';\nimport PlaylistItem from 'components/PlaylistItem';\nimport Scrollable from 'components/Scrollable';\nimport type { Track } from 'types';\n\ntype EnhancedTrack = {\n\tfoundInDescription?: boolean;\n} & Track;\n\ntype PlaylistItemsProps = {\n\tallowTwoColumns?: boolean;\n\tonSearchStateChange?: (processing: boolean) => void;\n\tsearch: string;\n\tshouldReverse: boolean;\n\tshowFeatured: boolean;\n} & withConfigProps &\n\twithPlayerStateProps;\n\nconst PlaylistItems: FC<PlaylistItemsProps> = ({\n\tallowTwoColumns = true,\n\tconfig,\n\tonSearchStateChange,\n\tplayerState: { playerWidth },\n\tsearch,\n\tshouldReverse,\n\tshowFeatured,\n}) => {\n\tconst [foundIds, setFoundIds] = useState<Array<number> | null>(null);\n\n\tconst abortControllerRef = useRef(new AbortController());\n\tconst onSearchStateChangeRef = useRef(onSearchStateChange);\n\tconst searchRef = useRef(search);\n\n\tonSearchStateChangeRef.current = onSearchStateChange;\n\tsearchRef.current = search;\n\n\tconst performSearchCall = useMemo(\n\t\t() =>\n\t\t\tdebounce(async (search: string, hash: string) => {\n\t\t\t\tonSearchStateChan